---
import { Image } from 'astro:assets';

interface Props {
    src: string;
    alt: string;
    class?: string;
    loading?: 'lazy' | 'eager';
}

const { 
    src, 
    alt,
    class: className = '',
    loading = 'lazy',
    ...props 
} = Astro.props;

export const profileImage = "https://media.licdn.com/dms/image/v2/D4E03AQFsRJCuxdTt9Q/profile-displayphoto-scale_400_400/B4EZnMwFAnHoAg-/0/1760076781380?e=1762387200&v=beta&t=6rQ-KvMLp4o4r2IDgGHd0KmjH-_NevQGwuA_-d5L6mE";
---

<Image alt={alt} src={src} class={className} loading={loading} height={100} width={100} {...props} />

